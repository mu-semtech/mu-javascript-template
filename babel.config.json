{
  "presets": [
    ["@babel/preset-env",
      {
        "targets": {
          "node": 16,
		  "esmodules": true
        },
        "modules": false
      }
    ],
    ["@babel/preset-typescript"]
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties"]
  ],
  "ignore": [
    "./node_modules",
    "/usr/src/processing/build/node_modules"
  ]
}
